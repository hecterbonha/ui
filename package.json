{
  "name": "@hecterbonha/ui",
  "version": "0.0.1",
  "description": "Yet Another React UI Library ",
  "scripts": {
    "build": "tsup",
    "test": "vitest"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "treeshake": true,
    "sourcemap": "inline",
    "minify": true,
    "clean": true,
    "dts": true,
    "splitting": false,
    "format": [
      "cjs",
      "esm"
    ],
    "external": [
      "react"
    ],
    "injectStyle": false
  },
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hecterbonha/ui.git"
  },
  "keywords": [
    "React",
    "UI"
  ],
  "author": "hecterbonha",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hecterbonha/ui/issues"
  },
  "homepage": "https://github.com/hecterbonha/ui#readme",
  "devDependencies": {
    "@types/react": "^18.2.31",
    "@types/react-dom": "^18.2.14",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.3",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}